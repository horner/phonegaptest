<?xml version='1.0' encoding='utf-8'?>
<widget id="io.cordova.enterprisehealth" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Enterprise Health</name>
    <description>
        Enterprise Health mobile app.
    </description>
    <author email="shott@mieweb.com" href="http://www.enterprisehealth.com">
        Enterprise Health Team
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-navigation href="*" />
    <allow-navigation href="https://webchartnow.com/apptest/" />
    <platform name="android">
        <allow-intent href="market:*" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="windows-target-version" value="10.0" />
    <preference name="windows-phone-target-version" value="10.0" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreenBackgroundColor" value="#f05c21" />
    <preference name="SplashScreenDelay" value="3000" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-splashscreen" spec="^5.0.2" />
    <engine name="osx" spec="^4.0.1" />
    <engine name="android" spec="^6.4.0" />
    <engine name="windows" spec="^5.0.0" />
    <engine name="ios" spec="^4.5.4" />
</widget>
